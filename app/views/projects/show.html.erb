<div data-role="content" class="project-show">
  <div data-role="collapsible-set">
    <%= render :partial => "todo" %>
    <div data-role="collapsible" data-collapsed="false">
      <%= render :partial => 'map' %>
    </div>
    <div data-role="collapsible" data-collapsed="false">
      <h3>
        Comments
      </h3>
      <div id="comments_container">
	<%= render :partial => 'comments' %>
      </div>
      <div id="new-comments">
      <%= form_for [@project, @comment] do |f|-%>
        <%= f.text_area :comment -%>
        <%= f.submit -%>
      <%- end -%>
      </div>
    </div>
    <div data-role="collapsible" data-collapsed="false">
      <h3>
        Twitter
      </h3>
      
    </div>
  </div>
</div>
